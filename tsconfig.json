{
    "compilerOptions": {
      "jsx": "react",
      "target": "ES2020",
      "module": "ESNext",
      "lib": ["DOM", "DOM.Iterable", "ESNext"],
      "declaration": true,
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
